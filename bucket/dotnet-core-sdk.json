{
    "version": "3.1.300",
    "homepage": "https://www.microsoft.com/net/",
    "description": ".NET is a free, cross-platform, open source developer platform for building many different types of applications.",
    "license": "MIT",
    "url": "https://raw.githubusercontent.com/dotnet/sdk/78d9adc9ea1d0f4634e29e7e07d0ecdf62e5dbce/scripts/obtain/dotnet-install.ps1#/_dotnet_install.ps1",
    "hash": "531d2c3caf0f8ec0c4fc91de4ae710e53cc9d456df61219f20a84639c70d128a",
    "bin": [
        [
            "pwsh.exe",
            "dotnet-install",
            "-File $dir\\_dotnet_install.ps1 -InstallDir $dir"
        ]
    ],
    "pre_install": [
        "if((Test-Path -Path $persist_dir\\bin\\dotnet.exe) -and !(Test-Path -Path $dir\\dotnet.exe)) { Copy-Item $persist_dir\\bin\\dotnet.exe -Destination $dir }"
    ],
    "post_install": [
        "dotnet-install -Channel 2.1",
        "dotnet-install -Channel 2.2",
        "dotnet-install -Version $version",
        "if(!(Test-Path -Path $persist_dir\\bin\\$version\\)) { New-Item -ItemType Directory -Force -Path $persist_dir\\bin\\$version\\ }",
        "if(!(Test-Path -Path $persist_dir\\bin\\$version\\dotnet.exe)) { Copy-Item $dir\\dotnet.exe -Destination $persist_dir\\bin\\$version\\ }",
        "Copy-Item $dir\\dotnet.exe -Destination $persist_dir\\bin\\ -Force"
    ],
    "depends": "pwsh",
    "suggest": {
        "Visual C++ Redistributable": [
            "extras/vcredist2015",
            "extras/vcredist2019"
        ]
    },
    "env_add_path": ".",
    "env_set": {
        "DOTNET_ROOT": "$dir"
    },
    "persist": [
        "additionalDeps",
        "host",
        "packs",
        "sdk",
        "shared",
        "store",
        "templates"
    ]
}
